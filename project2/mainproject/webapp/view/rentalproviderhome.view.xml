<mvc:View controllerName="com.applexus.mainproject.controller.rentalproviderhome"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:t="sap.ui.table"
    	xmlns:core="sap.ui.core" 
    xmlns:form="sap.ui.layout.form"
    xmlns="sap.m">
   
 <IconTabBar id="iconTabBar"
            class="sapUiNoContentPadding">
            <items>
               <IconTabFilter id="_IDGenIconTabFilter1" text="Post Homes" key="info" class="sapUiLargeMarginEnd">
                  
            <FlexBox
                id="_IDGenFlexBox1"
                width="100%"
                justifyContent="Center"
                alignItems="Center"
                height="100%"
            >
                <VBox id="_IDGenVBox2">
                    <FlexBox
                        id="_IDGenFlexBox2"
                        justifyContent="Center"
                        class="sapUiSmallMargin"
                    >
                        
                    </FlexBox>

                    <HBox id="_IDGenHBox2">
                        <VBox
                            id="_IDGenVBox3"
                            class="sapUiSmallMarginEnd"
                        >
                            <Label
                                id="_IDGenLabel2"
                                text="House/Flat No"
                            />

                            <Input
                                id="_IDGenInput1"
                                width="250px"
                            />
                        </VBox>

                        <VBox id="_IDGenVBox4">
                            <Label
                                id="_IDGenLabel3"
                                text="House/Flat Name"
                            />

                            <Input
                                id="_IDGenInput2"
                                width="250px"
                            />
                        </VBox>
                    </HBox>

                    <HBox id="_IDGenHBox3">
                        <VBox
                            id="_IDGenVBox5"
                            class="sapUiSmallMarginEnd"
                        >
                            <Label
                                id="_IDGenLabel4"
                                text="Street"
                            />

                            <Input
                                id="_IDGenInput3"
                                width="250px"
                            />
                        </VBox>

                        <VBox id="_IDGenVBox6">
                            <Label
                                id="_IDGenLabel5"
                                text="Place"
                            />

                            <Input
                                id="_IDGenInput4"
                                width="250px"
                            />
                        </VBox>
                    </HBox>

                    <HBox id="_IDGenHBox4">
                        <VBox
                            id="_IDGenVBox7"
                            class="sapUiSmallMarginEnd"
                        >
                            <Label
                                id="_IDGenLabel6"
                                text="District"
                            />

                            <Input
                                id="_IDGenInput5"
                                width="250px"
                            />
                        </VBox>

                        <VBox id="_IDGenVBox8">
                            <Label
                                id="_IDGenLabel7"
                                text="Pincode"
                            />

                            <Input
                                id="_IDGenInput6"
                                width="250px"
                            />
                        </VBox>
                    </HBox>

                    <HBox id="_IDGenHBox5">
                        <VBox
                            id="_IDGenVBox9"
                            class="sapUiSmallMarginEnd"
                        >
                            <Label
                                id="_IDGenLabel8"
                                text=" No of Bedrooms"
                            />

                            <Input
                                id="_IDGenInput7"
                                width="250px"
                            />
                        </VBox>

                        <VBox id="_IDGenVBox10">
                            <Label
                                id="_IDGenLabel9"
                                text="No of Persons allowed"
                            />

                            <Input
                                id="_IDGenInput8"
                                width="250px"
                            />
                        </VBox>
                    </HBox>

                    <HBox id="_IDGenHBox6">
                        <VBox
                            id="_IDGenVBox11"
                            class="sapUiSmallMarginEnd"
                        >
                            <Label
                                id="_IDGenLabel10"
                                text="Furnishing"
                            />

                            <Input
                                id="_IDGenInput9"
                                width="250px"
                            />
                        </VBox>

                        <VBox id="_IDGenVBox12">
                            <Label
                                id="_IDGenLabel11"
                                text="Rent Amount"
                            />

                            <Input
                                id="_IDGenInput12"
                                width="250px"
                            />
                        </VBox>
                    </HBox>

                    <HBox id="_IDGenHBox7">
                        <VBox
                            id="_IDGenVBox13"
                            class="sapUiSmallMarginEnd"
                        >
                            <Label
                                id="_IDGenLabel13"
                                text="Owner Name"
                            />

                            <Input
                                id="_IDGenInput13"
                                width="250px"
                            />
                        </VBox>

                        <VBox id="_IDGenVBox14">
                            <Label
                                id="_IDGenLabel14"
                                text="Phone Number"
                            />

                            <Input
                                id="_IDGenInput14"
                                width="250px"
                            />
                        </VBox>
                    </HBox>

                    <HBox id="_IDGenHBox8">
                        <VBox
                            id="_IDGenVBox15"
                            class="sapUiSmallMarginEnd"
                        >
                            <Label
                                id="_IDGenLabel15"
                                text="Email Id"
                            />

                            <Input
                                id="_IDGenInput15"
                                width="250px"
                            />
                        </VBox>

                        <VBox id="_IDGenVBox16">
                            <Label
                                id="_IDGenLabel16"
                                text="Upload images"
                            />

                            <Input
                                id="_IDGenInput16"
                                width="250px"
                            />
                        </VBox>
                    </HBox>

                    
                    <HBox
                        id="_IDGenVBox19"
                        justifyContent="Center"
                        class="sapUiSmallMarginTop"
                    >
                        <Button
                            id="_IDGenButton1"
                            text="Post"
                            press="onPress"
                        />
                    </HBox>
                </VBox>
            </FlexBox>
               </IconTabFilter>
               <IconTabFilter id="_IDGenIconTabFilter2" text="View Bookings" key="statistics">
               <FlexBox
                        id="_IDGenFlexBox3"
                        justifyContent="Center"
                        class="sapUiSmallMargin"
                    >
                        
                    </FlexBox>
                <Table id="_IDGenTable1"  items="{path:'requests>/booking'}" ariaLabelledBy="title">

    <columns>

        <Column id="_IDGenColumn1"  width="5rem">

            <Label id="_IDGenLabel21"  text="House No" />

        </Column>

        <Column id="_IDGenColumn2"  width="5rem">

            <Label id="_IDGenLabel22"  text="House Name" />

        </Column>

        <Column id="_IDGenColumn3"  width="5rem">

            <Label id="_IDGenLabel23"  text="Status" />

        </Column>

        <Column id="_IDGenColumn4"  width="5rem">

            <Label id="_IDGenLabel24"  text="Booked by" />

        </Column>

        <Column id="_IDGenColumn5"  width="5rem">

            <Label id="_IDGenLabel25"  text="Address" />

        </Column>
        <Column id="_IDGenColumn6"  width="5rem">

            <Label id="_IDGenLabel26"  text="Phone Number" />

        </Column>
         <Column id="_IDGenColumn7"  width="5rem">

            <Label id="_IDGenLabel27"  text="Change Status" />

        </Column>

        </columns>

        <items>

            <ColumnListItem id="_IDGenColumnListItem1" >

                <cells>

                    <Text id="_IDGenText1"  text="{requests>Employee id}"/>

                </cells>

                <cells>

                    <Text id="_IDGenText2"  text="{requests>Employee Name}"/>

                </cells>

                <cells>

                    <Text id="_IDGenText3"  text="{requests>Company Name}"/>

                </cells>

                <cells>

                    <Text id="_IDGenText4"  text="{requests>Booked Address}"/>

                </cells>
                <cells>

                    <Text id="_IDGenText5"  text="{requests>Owner Name}"/>

                </cells>
                <cells>

                    <Text id="_IDGenText6"  text="{requests>Booking Date}"/>

                </cells>
                <cells>

                     <Button id="btn2" text="Change" press="onchangePress"/>    

                </cells>
               

            </ColumnListItem>

        </items>

    </Table>
                      
               </IconTabFilter>
               <IconTabFilter id="_IDGenIconTabFilter3" text="Update House Details"  key="info" >
               
            <Table id="_IDGenTable2"  items="{path:'requests>/booking'}" ariaLabelledBy="title">

    <columns>

        <Column id="_IDGenColumn8"  width="5rem">

            <Label id="_IDGenLabel28"  text="House/Flat No" />

        </Column>

        <Column id="_IDGenColumn9"  width="5rem">

            <Label id="_IDGenLabel29"  text="House/Flat Name" />

        </Column>
        <Column id="_IDGenColumn10"  width="5rem">

            <Label id="_IDGenLabel30"  text="Action" />

        </Column>
        </columns>
        
        <items>
         <ColumnListItem id="_IDGenColumnListItem2" >
          <cells>

                    <Text id="_IDGenText7"  text="{requests>Employee id}"/>

                </cells>

                <cells>

                    <Text id="_IDGenText8"  text="{requests>Employee Name}"/>

                </cells>

 <cells>

                     <Button id="btn3" text="Update" press="onUpdatePress"/>    

                </cells>
               

            </ColumnListItem>

        </items>

    </Table>
               </IconTabFilter>
            </items>
         </IconTabBar>
        
         
</mvc:View>