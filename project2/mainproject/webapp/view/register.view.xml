<mvc:View
    controllerName="com.applexus.mainproject.controller.register"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m"
>
    <Page
        id="page"
        title="{i18n>title}"
    >
        <content>
            <HBox id="_IDGenHBox1">
                <VBox
                    id="_IDGenVBox1"
                    class="sapUiSmallMargin"
                >
                    <Title
                        text=""
                        id="_IDGenTitle1"
                        level="H1"
                    />
                </VBox>
            </HBox>

            <FlexBox
                id="_IDGenFlexBox1"
                width="100%"
                justifyContent="Center"
                alignItems="Center"
                height="100%"
            >
                <VBox id="_IDGenVBox2">
                    <FlexBox
                        id="_IDGenFlexBox2"
                        justifyContent="Center"
                        class="sapUiSmallMargin"
                    >
                        <Label
                            id="_IDGenLabel1"
                            text="Registration Form"
                            design="Bold"
                        />
                    </FlexBox>

                    <HBox id="_IDGenHBox2">
                        <VBox
                            id="_IDGenVBox3"
                            class="sapUiSmallMarginEnd"
                        >
                            <Label
                                id="_IDGenLabel2"
                                text="First Name"
                            />

                            <Input
                                id="_IDGenInput1"
                                width="250px"
                            />
                        </VBox>

                        <VBox id="_IDGenVBox4">
                            <Label
                                id="_IDGenLabel3"
                                text="Last Name"
                            />

                            <Input
                                id="_IDGenInput2"
                                width="250px"
                            />
                        </VBox>
                    </HBox>

                    <HBox id="_IDGenHBox3">
                        <VBox
                            id="_IDGenVBox5"
                            class="sapUiSmallMarginEnd"
                        >
                            <Label
                                id="_IDGenLabel4"
                                text="Email Id"
                            />

                            <Input
                                id="_IDGenInput3"
                                width="250px"
                            />
                        </VBox>

                        <VBox id="_IDGenVBox6">
                            <Label
                                id="_IDGenLabel5"
                                text="Phone Number"
                            />

                            <Input
                                id="_IDGenInput4"
                                width="250px"
                            />
                        </VBox>
                    </HBox>

                    <HBox id="_IDGenHBox4">
                        <VBox
                            id="_IDGenVBox7"
                            class="sapUiSmallMarginEnd"
                        >
                            <Label
                                id="_IDGenLabel6"
                                text="House Name"
                            />

                            <Input
                                id="_IDGenInput5"
                                width="250px"
                            />
                        </VBox>

                        <VBox id="_IDGenVBox8">
                            <Label
                                id="_IDGenLabel7"
                                text="Street"
                            />

                            <Input
                                id="_IDGenInput6"
                                width="250px"
                            />
                        </VBox>
                    </HBox>

                    <HBox id="_IDGenHBox5">
                        <VBox
                            id="_IDGenVBox9"
                            class="sapUiSmallMarginEnd"
                        >
                            <Label
                                id="_IDGenLabel8"
                                text=" Place"
                            />

                            <Input
                                id="_IDGenInput7"
                                width="250px"
                            />
                        </VBox>

                        <VBox id="_IDGenVBox10">
                            <Label
                                id="_IDGenLabel9"
                                text="Pincode"
                            />

                            <Input
                                id="_IDGenInput8"
                                width="250px"
                            />
                        </VBox>
                    </HBox>

                    <HBox id="_IDGenHBox6">
                        <VBox
                            id="_IDGenVBox11"
                            class="sapUiSmallMarginEnd"
                        >
                            <Label
                                id="_IDGenLabel10"
                                text="District"
                            />

                            <Input
                                id="_IDGenInput9"
                                width="250px"
                            />
                        </VBox>

                        <VBox id="_IDGenVBox12">
                            <Label
                                id="_IDGenLabel11"
                                text="State"
                            />

                            <Input
                                id="_IDGenInput12"
                                width="250px"
                            />
                        </VBox>
                    </HBox>

                    <HBox id="_IDGenHBox7">
                        <VBox
                            id="_IDGenVBox13"
                            class="sapUiSmallMarginEnd"
                        >
                            <Label
                                id="_IDGenLabel13"
                                text="Phase"
                            />

                            <Input
                                id="_IDGenInput13"
                                width="250px"
                            />
                        </VBox>

                        <VBox id="_IDGenVBox14">
                            <Label
                                id="_IDGenLabel14"
                                text="Company"
                            />

                            <Input
                                id="_IDGenInput14"
                                width="250px"
                            />
                        </VBox>
                    </HBox>

                    <HBox id="_IDGenHBox8">
                        <VBox
                            id="_IDGenVBox15"
                            class="sapUiSmallMarginEnd"
                        >
                            <Label
                                id="_IDGenLabel15"
                                text="Employee ID"
                            />

                            <Input
                                id="_IDGenInput15"
                                width="250px"
                            />
                        </VBox>

                        <VBox id="_IDGenVBox16">
                            <Label
                                id="_IDGenLabel16"
                                text="Mail ID"
                            />

                            <Input
                                id="_IDGenInput16"
                                width="250px"
                            />
                        </VBox>
                    </HBox>

                    <HBox id="_IDGenHBox9">
                        <VBox
                            id="_IDGenVBox17"
                            class="sapUiSmallMarginEnd"
                        >
                            <Label
                                id="_IDGenLabel17"
                                text="Password"
                            />

                            <Input
                                id="_IDGenInput17"
                                width="250px"
                                type="Password"
                            />
                        </VBox>

                        <VBox id="_IDGenVBox18">
                            <Label
                                id="_IDGenLabel18"
                                text="Confirm Password"
                            />

                            <Input
                                id="_IDGenInput18"
                                width="250px"
                                type="Password"
                            />
                        </VBox>
                    </HBox>

                    <HBox
                        id="_IDGenVBox19"
                        justifyContent="Center"
                        class="sapUiSmallMarginTop"
                    >
                        <Button
                            id="_IDGenButton1"
                            text="Submit"
                            press="onPress"
                        />
                    </HBox>
                </VBox>
            </FlexBox>
        </content>
    </Page>
</mvc:View>
