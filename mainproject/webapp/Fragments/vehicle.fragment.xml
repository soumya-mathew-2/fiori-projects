<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form">
  <IconTabBar
    stretchContentHeight="true"
    class="sap.m.IconTabBar"
    id="_IDGenIconTabBar8"
    enableTabReordering="true"
    headerMode="Inline"
    upperCase="true"
  >
    <items>
      <IconTabFilter
        id="Icon1"
        key="info"
        icon="sap-icon://car-rental"
        text="Add Vehicle"
        design="Horizontal"
        tooltip="Login"
      >
        
          <f:SimpleForm id="mySimpleForm" editable="true" class="customSimpleForm">
            <Label id="_IDGenLabel61" text="Vehicle no" />
            <Input id="_IDGenInput61" width="40%" />

            <Label id="_IDGenLabel62" text="Vehicle Type" />
            <Input id="_IDGenInput62" width="40%" />

            <Label id="_IDGenLabel63" text="Vehicle Name" />
            <Input id="_IDGenInput63" width="40%" />

            <Label id="_IDGenLabel64" text="Fuel Type" />
            <Input id="_IDGenInput64" width="40%" />

            <Label id="_IDGenLabel65" text="Number Of Seats" />
            <Input id="_IDGenInput65" width="40%" />

          </f:SimpleForm>
<VBox id="_IDGenVBox5" alignItems="Center" justifyContent="Center">
          <Button id="_IDGenButton1" text="Add" press="onAdd"  />
          </VBox>
        
      </IconTabFilter>
    </items>
  </IconTabBar>
</core:FragmentDefinition>
